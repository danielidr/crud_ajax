<p id="notice"><%= notice %></p>
<div id="createData"></div>

<div class="row">
    <h1 class="pt-4 col-9">Posts</h1>
    <%= render('posts/search') %>
</div>
<table class="table table-striped">
    <thead>
        <tr>
        <th scope="col">Title</th>
        <th scope="col">Content</th>
        <th colspan="3"></th>
        </tr>
    </thead>

    <tbody id="data">
        <% @posts.each do |p| %>
            <%= render 'data', p: p %>
        <% end %>
    </tbody>
</table>

<br>

<%= link_to 'New Post', new_post_path, class: "btn btn-secondary", remote: true %>

<div id="showData"></div>